library(tidyverse)
library(marginaleffects)
library(reticulate)
library(kableExtra)
N <- 97
x <- rnorm(N)
y <- rbinom(N, 1,plogis(-0.8 + 0.45*x))
df <- tibble(x, y)
fit <- glm(y~x, data=df, family = binomial())
marginaleffects(fit) %>%
summary() %>%
kbl() %>%
kable_styling(bootstrap_options = c('floating', 'striped'))
reticulate::repl_python()
reticulate::repl_python()
